{
  "name": "paubox-node",
  "version": "1.2.5",
  "description": "A Node.js module for the Paubox Transactional Email API.",
  "main": "index.js",
  "engines": {
    "node": "~22.16.0 || ^24.0.0",
    "npm": ">=10.9.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha --reporter spec",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write '**/*.{js,json,md}'",
    "clean": "rm -rf lib/",
    "build": "npm run clean && babel src -d lib && npm run format"
  },
  "author": "Paubox <info@paubox.com> (http://www.paubox.com)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:Paubox/paubox-node.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^8.0.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^8.3.2",
    "papaparse": "^5.3.0",
    "prettier": "^2.8.8",
    "sinon": "^20.0.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "eslintIgnore": [
    ".gitignore",
    "src",
    "lib"
  ],
  "dependencies": {
    "axios": "1.9.0",
    "dotenv": "^6.2.0"
  }
}
